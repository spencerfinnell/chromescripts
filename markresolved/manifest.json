{
    "manifest_version" : 2,
    "name" : "MarkResolved",
    "description" : "Mark a topic as resolved",
    "version": "1.0",

    "permissions" : ["http://*/*"],

    "content_scripts":[{
        "matches" : ["http://*/*"],
        "js" : ["jquery.js", "jquery.hotkeys.js", "resolved.js"]
    }]
}
